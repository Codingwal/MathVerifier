theorem A2(P; Q; R):
    P => (Q => R)
=>  (P => Q) => (P => R)
{
    sorry
}

theorem T1(P; Q; R):
    P => Q
    Q => R
=>  P => R
{
    P => (Q => R)
    (P => Q) => (P => R)    | A2(P; Q; R)
    P => R      
}

theorem SpecificQ(M; B; P):
    ∀ x (x ∈ M ⇒ P)
    B ⊆ M
⇒  ∀ x (x ∈ B ⇒ P)
{
        ∀ x (x ∈ B ⇒ x e M)
        ∀ x ((x ∈ B ⇒ x ∈ M) ∧ (x ∈ M) ⇒ P) | T1(x ∈ B; x ∈ M; P)
        ∀ x (x ∈ B ⇒ P)
}