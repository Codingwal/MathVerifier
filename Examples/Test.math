define 0:
end

define Succ:
    ∀n(∀m(Succ(n) = Succ(m) ⇒ n = m))
    ¬∃n(Succ(n) = 0)
end

define ℕ:
    0 ∈ ℕ
    ∀ n(n ∈ ℕ ⇒ Succ(n) ∈ ℕ)
    ∀ ϕ((ϕ(0) ∧ ∀ n(ϕ(n) ⇒ ϕ(Succ(n)))) ⇒ ∀ n(ϕ(n)))
end

define 1:
    1 = Succ(0)
end

theorem 1_in_ℕ(P, Q, R, S):
⇒ 1 ∈ ℕ
{
    ∀ n(n ∈ ℕ ⇒ Succ(n) ∈ ℕ)   | @ℕ
    0 ∈ ℕ ⇒ Succ(0) ∈ ℕ        | sorry
    Succ(0) ∈ ℕ                 | @ℕ
    1 = Succ(0)                  | @1
    1 ∈ ℕ                       | sorry
}
