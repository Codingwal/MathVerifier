theorem A2(P; Q; R):
    P => (Q => R)
=>  (P => Q) => (P => R)
{
    sorry
}

theorem T1(P; Q; R):
    P => Q
    Q => R
=>  P => R
{
    P => (Q => R)
    (P => Q) => (P => R)    | A2(P; Q; R)
    P => R      
}

theorem SpecificQ(M; B; P):
    A x (x e M => P)
    B is M
=>  A x (x e B => P)
{
    A x (x e B => x e M)
    A x ((x e B => x e M) AND (x e M) => P) | T1(x e B; x e M; P)
    A x (x e B => P)
}